<div class="athlete-feed-entry athlete-feed-entry--drartikel {{#if breaking}}athlete-feed-entry--breaking{{/if}}">
	{{>athleteFeedEntryHeader target}}
	<div class="athlete-feed-entry__content">
		<div class="athlete-feed-entry__source"><span>Artikel om {{target.firstName}} fra </span><h4 class="athlete-profile-social-label athlete-profile-social-label--drartikel">DR Sporten</h4></div>
		<p>{{{data.content}}}</p>
		{{#if unread}}
			<div class="athlete-feed-entry__new-indicator"></div>
		{{/if}}
		<div class="athlete-feed-entry__time">
			{{time}}
		</div>
	</div>
</div>
