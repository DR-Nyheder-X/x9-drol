<div class="list-block contacts-block athlete-list">
	{{#each .}}
		<div class="list-group">
			<ul>
				<li class="list-group-title" data-index-letter="{{letter}}">{{letter}}</li>
				{{#each athletes}}
					<li>
						<div class="item-content">
							<div class="item-inner">
								<div class="item-title">
									<a href="pages/athlete-profile.html?id={{id}}">{{name}}</a>
									<div class="toggle-button {{#if following}} toggle-button--yes {{else}} toggle-button--no {{/if}}" data-athlete="{{id}}"></div>
								</div>
							</div>
						</div>
					</li>
				{{/each}}
			</ul>
		</div>
	{{/each}}
</div>
