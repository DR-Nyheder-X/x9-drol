{
  "name": "x9",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "npm run clean && npm run build:f7 && npm run build:js",
    "build:dev": "npm run clean && npm run build:f7 && npm run build:js-watch",
    "build:f7": "cp -R external/* dist/",
    "build:css": "lessc src/less/styles.less dist/styles.css",
    "build:js": "babel --presets es2015 src/js/index.js -o dist/index.js",
    "build:js-watch": "babel --presets es2015 src/js/index.js -o dist/index.js --watch"
  },
  "dependencies": {
    "babel-cli": "6.3.17",
    "babel-core": "6.3.26",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "6.3.13",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "handlebars": "^3.0.0",
    "handlebars-loader": "^1.0.2",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13"
  },
  "devDependencies": {}
}
